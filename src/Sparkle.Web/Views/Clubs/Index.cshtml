@model List<Sparkle.Services.Networks.Models.ClubModel>
@{
    ViewBag.Title = Lang.T("ClubsLabel");
}
@{ this.SetAccentColor("#038a9a"); }

@section CustomFocus{
    <input class="filterBox" type="text" value="" placeholder="@Lang.T("Rechercher")" onkeyup="SparkleNetworks.Clubs.FilterBoxSearch(this.value)" />
    <div style="float: right;margin: 2px 10px 0 0;">
        <a class="button accentColor" href="@Url.Action("New")" style="height:17px;" title="@Lang.T("Nouveau")"><img src="/Content/icones/appbar.add.light.png" alt="+" style="width: 18px;" /></a>
    </div>
    @FocusPage.Custom(ViewBag.Title)
}
<div class="space20"></div>

<div id="clubsResult" class="tile">
@if(Model.Count > 0) {
@Html.Partial("Lists/ClubsList", Model) 
} else {
<div class="introducing-title">@Lang.T("Aucun club n'est actuellement présent.")</div>
}
</div>

<script type="text/javascript">
    menu('mainNavClubs');
</script>